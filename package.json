{
  "name": "fl-9-team-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-double"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "firebase": "^5.5.6",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.4",
    "prettier": "^1.14.3"
  },
  "lint-staged": {
    "src/**/*.{js, jsx}": [
      "node_modules/.bin/eslint --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ]
  }
}
